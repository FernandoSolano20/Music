<GridPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.musica.ui.views.SongQueue.SongQueue">
    <TableView fx:id="table" prefHeight="369.0" prefWidth="619.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
        <columns>
            <TableColumn fx:id="columnName" prefWidth="116.0" text="Nombre" />
            <TableColumn fx:id="columnScore" prefWidth="97.0" text="Calificación" />
            <TableColumn fx:id="columnGender" prefWidth="120.0" text="Género" />
            <TableColumn fx:id="columnArt" prefWidth="115.0" text="Artista" />
         <TableColumn fx:id="columnAdd" prefWidth="91.0" text="Añadir" />
            <!-- and so on... -->
        </columns>
    </TableView>
    <Button mnemonicParsing="false" onAction="#buy" text="Comprar" translateX="65.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Button mnemonicParsing="false" onAction="#lists" prefHeight="31.0" prefWidth="86.0" text="Listas" translateX="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Button fx:id="catalogs" mnemonicParsing="false" onAction="#catalogs" text="Catalogos" translateX="245.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Button mnemonicParsing="false" onAction="#albums" text="Albumes" translateX="340.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Button mnemonicParsing="false" onAction="#reproductions" text="Cola de reproduccion" translateX="430.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Button mnemonicParsing="false" onAction="#index" text="Inicio" translateX="3.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
   <TextField fx:id="search" maxWidth="150.0" minWidth="10.0" prefHeight="31.0" prefWidth="150.0" translateX="95.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
   <Button fx:id="btnSearch" mnemonicParsing="false" text="Buscar" GridPane.columnIndex="1" GridPane.rowIndex="2" />
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
      <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
      <RowConstraints />
    </rowConstraints>
</GridPane>
