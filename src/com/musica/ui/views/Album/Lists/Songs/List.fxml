<GridPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.musica.ui.views.Album.Lists.Songs.ListSongAlbum">
    <TableView fx:id="table" prefHeight="379.0" prefWidth="600.0" GridPane.rowIndex="3">
        <columns>
            <TableColumn fx:id="columnName" prefWidth="219.0" text="Nombre" />
            <TableColumn fx:id="columnDelete" prefWidth="124.0" text="Borrar" />
            <!-- and so on... -->
        </columns>
    </TableView>
    <Button mnemonicParsing="false" onAction="#rComp" text="Compositor" translateX="160.0" GridPane.rowIndex="1" />
    <Button mnemonicParsing="false" onAction="#rArt" prefHeight="31.0" prefWidth="86.0" text="Artistas" translateX="300.0" GridPane.rowIndex="1" />
