<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.musica.ui.views.Artist.Lists.ListArtist">
    <TableView fx:id="table" prefHeight="379.0" prefWidth="600.0" GridPane.rowIndex="3">
        <columns>
            <TableColumn fx:id="columnName" prefWidth="156.0" text="Nombre" />
            <TableColumn fx:id="columnCountry" prefWidth="98.0" text="Pais" />
         <TableColumn fx:id="columnArtistName" prefWidth="137.0" text="Alias" />
            <TableColumn fx:id="columnEdit" prefWidth="92.0" text="Editar" />
            <TableColumn fx:id="columnDelete" prefWidth="117.0" text="Borrar" />
            <!-- and so on... -->
        </columns>
    </TableView>
    <Button mnemonicParsing="false" onAction="#rComp" text="Compositor" translateX="160.0" GridPane.rowIndex="1" />
    <Button mnemonicParsing="false" onAction="#rArt" prefHeight="31.0" prefWidth="86.0" text="Artistas" translateX="300.0" GridPane.rowIndex="1" />
    <Button fx:id="catalogs" mnemonicParsing="false" onAction="#listSong" prefHeight="31.0" prefWidth="84.0" text="Cancion" translateX="400.0" GridPane.rowIndex="1" />
    <Button mnemonicParsing="false" onAction="#rAlbum" text="Album" translateX="500.0" GridPane.rowIndex="1" />
    <Button mnemonicParsing="false" onAction="#rGender" text="Generos" translateX="50.0" GridPane.rowIndex="1" />
    <TextField fx:id="search" maxWidth="231.0" prefHeight="31.0" prefWidth="208.0" translateX="100.0" GridPane.rowIndex="2" />
    <Label text="Buscar" GridPane.rowIndex="2" />
    <Button mnemonicParsing="false" onAction="#cArt" text="Crear" translateX="500.0" GridPane.rowIndex="2" />
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
</GridPane>
